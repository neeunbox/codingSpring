<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Exam exam = new NewlecExam(); -->
    <bean id="exam" class="com.studies.spring.di.entity.NewlecExam">
        <property name="kor" value="80" />     
        <property name="eng" value="80" />
        <property name="math" value="80" />
        <property name="com" value="80" />                           
    </bean>

    <!-- ExamConsole console = new GridExamConsole(); -->
    <bean id="console" class="com.studies.spring.di.ui.GridExamConsole">
        <!-- console.setExam(exam);     -->
        <property name="exam" ref="exam" />
    </bean>

</beans>